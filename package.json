{
  "name": "@utech/born-ui",
  "version": "1.0.2-alpha.6",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build && node ./scripts/copy-antd",
    "release": "npm run build && npm publish",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\""
  },
  "main": "./es/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "es",
    "package.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "author": {
    "name": "王建明",
    "email": "wangjianming.x@gmail.com"
  },
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "http://8.136.101.171:88"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "dependencies": {
    "@ahooksjs/use-url-state": "^2.5.1",
    "@ant-design/icons": "^4.2.2",
    "@utech/react-sortable-hoc": "^1.11.1",
    "ahooks": "^2.6.0",
    "antd": "^4.11.1",
    "array-move": "^3.0.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.36",
    "fast-deep-equal": "^3.1.3",
    "lodash": "^4.17.20",
    "numeral": "^2.0.6",
    "unstated-next": "^1.1.0",
    "use-json-comparison": "^1.0.5",
    "use-merge-value": "^1.0.2"
  },
  "devDependencies": {
    "@ant-design/pro-skeleton": "^1.0.0-beta.9",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/jest": "^26.0.19",
    "@types/numeral": "^0.0.28",
    "@umijs/fabric": "^2.5.6",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.0",
    "commitlint": "^11.0.0",
    "dumi": "^1.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.9.0",
    "father-build": "^1.17.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.3.0",
    "mockdate": "^3.0.2",
    "prettier": "^2.1.2",
    "react-dom": "^16.13.1",
    "stylelint": "^13.7.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-router": "^5.0.0"
  }
}
